set -a && source .env && set +a && \
vercel link --yes --project firebase-vercel --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_API_KEY development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_API_KEY" | vercel env add FIREBASE_API_KEY development --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_APP_ID development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_APP_ID" | vercel env add FIREBASE_APP_ID development --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_AUTH_DOMAIN development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_AUTH_DOMAIN" | vercel env add FIREBASE_AUTH_DOMAIN development --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_MESSAGING_SENDER_ID development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_MESSAGING_SENDER_ID" | vercel env add FIREBASE_MESSAGING_SENDER_ID development --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_PROJECT_ID development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_PROJECT_ID" | vercel env add FIREBASE_PROJECT_ID development --token $VERCEL_TOKEN && \
vercel env rm FIREBASE_STORAGE_BUCKET development --yes --token $VERCEL_TOKEN && \
echo "$FIREBASE_STORAGE_BUCKET" | vercel env add FIREBASE_STORAGE_BUCKET development --token $VERCEL_TOKEN && \
vercel dev --token $VERCEL_TOKEN
